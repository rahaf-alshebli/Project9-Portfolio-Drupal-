<!-- Testimonial items  -->
	<div class="col-md-6 mx-auto text-center testimonial-content wow fadeInDown">
	  <div class="single-testimonial-carousel">
	    <div class="testimonial-information">
	      <div class="testimonial-image">
	       <img src="{{ file_url(content.field_image['#items'].entity.uri.value) }}"  class="img-responsive" alt="{{ content.field_image['#items'].alt }}" title="{{ content.field_image['#items'].title }}">

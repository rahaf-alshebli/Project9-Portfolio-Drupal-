<!-- Contact section  -->
  <section id="contact" style="background-image:url('{{ file_url(content.field_image['#items'].entity.uri.value) }}');background-size: cover;background-repeat: no-repeat;">
    <div class="container">
      <div class="row">
        <div class="col-md-6">
        <div class="contact-info wow fadeInUp" data-wow-delay="0.3s">
        {% if content.field_address.0|render is not empty %}
          <h3>{{ content.field_address['#title'] }}</h3>
          <p>{{ content.field_address.0 }}</p>
        {% endif %}  
        </div>  
        <div class="contact-info wow fadeInUp" data-wow-delay="0.6s">
        {% if content.field_telephone.0|render is not empty %}
          <h3>{{ content.field_telephone['#title'] }}</h3>
          <p>{{ content.field_telephone }}</p>
        {% endif %} 
        </div> 
        <div class="contact-info wow fadeInUp" data-wow-delay="0.9s">
        {% if content.field_email.0|render is not empty %}
          <h3>{{ content.field_email['#title'] }}</h3>
          <p><a href="mailto:{{ content.field_email.0 }}">{{ content.field_email.0 }}</a></p>
        {% endif %}  
        </div> 
        </div>
        <div class="col-md-6 wow fadeInUp" data-wow-delay="0.5s">
          <h1>{{ content.field_contact['#title'] }}</h1>
          	{{ content.field_contact.0 }}
        </div>

      </div>  
    </div>    
</section>  
<!-- End Contact section  -->
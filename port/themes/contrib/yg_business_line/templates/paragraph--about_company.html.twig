        <h2>{{content.field_second_title.0}}</h2>
        <p>{{content.field_second_description.0}}</p>
      </div>
      <div class="col-md-7 col-xs-12 video-section wow fadeInLeft">
        <iframe width="600" height="315" src="{{content.field_video_url.0}}" frameborder="0" allow="autoplay; encrypted-media" allowfullscreen></iframe>  

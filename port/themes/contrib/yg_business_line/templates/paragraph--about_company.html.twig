    <div class="row">
      <div class="col-md-5 col-xs-12 wow fadeInLeft">
        <img src="{{content.field_image.0}}" class="img-responsive" alt="Title">  
      </div>      
      <div class="col-md-7 col-xs-12 details-section wow fadeInLeft">
        <h2>{{content.field_title.0}}</h2>
        <p>{{content.field_description.0}}</p>
      </div>
    </div>
  </div>  
</section>
<section id="about-section-2">
  <div class="container">
    <div class="row">      

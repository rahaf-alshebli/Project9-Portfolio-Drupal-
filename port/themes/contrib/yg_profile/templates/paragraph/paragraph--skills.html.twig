 <div class="section light-bg" id="skills">
        <div class="container">
            <div class="section-title">
                <small class="wow slideInUp">{{ content.field_about_sub_title.0 }}</small>
                <h3 class="wow slideInUp">{{ content.field_about_title.0 }}</h3>
            </div>
            <div class="row">                                      
                <div class="col-12 col-lg-6">                  
                    <div class="about-me">                    
                        <h4 class="wow slideInUp">{{ content.field_email.0 }}</h4>
                        {{ content.field_about_body_desc.0 }}
                    </div>
                    <div class="contact-me">                        
                        <a href="{{ content.field_banner_btn[0]['#url']|render }}" class="btn btn-light wow slideInUp">{{ content.field_banner_btn[0]['#title']|render }}</a>
                    </div>
                </div>
                <div class="col-12 col-lg-6">
                     {{ content.field_skills_lists }}                  
                </div>             
            </div>
        </div>
    </div> 